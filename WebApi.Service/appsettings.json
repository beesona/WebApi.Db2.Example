{
    "AppSettings": {
        "ApplicationName": "WebApi.Service",
        "VersionId": "v0.0",
        "DbConnectionString": "database={DATABASE};user id={USER_ID};password={PASSWORD};server={SERVER_IP:PORT};persist security info=True;CurrentSchema={SCHEMA}"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*"
}
